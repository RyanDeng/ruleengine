<?xml version="1.0" encoding="UTF-8"?>
<package name="cn.dc">
<import name="cn.dc.ruleengine.UserAccount"/>
<import name="cn.dc.ruleengine.Account"/>
<rule name="account">
	<rule-attribute name="salience" value="10" />
	<when>
		<column type="UserAccount" name="userAccount">
			<condition>  userAccount.userId>2</condition>
			<condiiton >userAccount.age>12</condiiton>
		</column>
		<column type="Account" name="account">
			<condition>account.userId==userAccount.userId</condition>
		</column>
	</when>
	<then>
		System.out.println(account.getBalance());
	</then>
</rule>
</package>